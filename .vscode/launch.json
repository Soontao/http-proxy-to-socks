{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "launch-hsts",
      "program": "${workspaceFolder}/bin/hpts.js",
      "env": {
        "DISABLE_CLUSTER": "true",
        "LOGGER_LEVEL": "info"
      },
      "args": [
        "--port",
        "18080",
        "--host",
        "0.0.0.0",
        "--socks",
        "192.168.3.88:10080",
        "--level",
        "info"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "vscode-jest-tests",
      "request": "launch",
      "args": [
        "--runInBand"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "program": "${workspaceFolder}/node_modules/jest/bin/jest"
    }
  ]
}