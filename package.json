{
  "name": "http-socks",
  "version": "1.1.3",
  "description": "hpts (http-proxy-to-socks) is a nodejs client to convert socks proxy into http proxy",
  "main": "srv/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/http-proxy-to-socks"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "keywords": [
    "socks",
    "http proxy",
    "converting proxy"
  ],
  "bin": {
    "http-socks": "./bin/http-socks.js",
    "hpts": "./bin/hpts.js"
  },
  "author": "oyyd <oyydoibh@gmail.com>",
  "contributors": [
    {
      "name": "Theo Sun",
      "email": "theo.sun@outlook.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "adblockpluscore": "github:adblockplus/adblockpluscore#adblockpluschrome-3.9",
    "commander": "^2.9.0",
    "lodash": "^4.17.15",
    "log4js": "^6.2.1",
    "netmask": "^1.0.6",
    "socks": "^1.1.10",
    "workerpool": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node-fetch": "^2.5.7",
    "eslint": "^7",
    "jest": "^26"
  },
  "engines": {
    "node": ">=12"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/net.js"
    ],
    "testRegex": "(\\.|/)(test|spec)\\.js$"
  }
}
